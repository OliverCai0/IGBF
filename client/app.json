{
  "expo": {
    "name": "IGBF",
    "slug": "IGBF",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IGBF-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/IGBF-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oliver0.IGBF"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IGBF-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
